/* === Estilo general moderno === */
QMainWindow {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1, stop:0 #232526, stop:1 #414345);
    color: #FFFFFF;
    font-family: "Inter", "Segoe UI Variable", "Roboto", "Open Sans", sans-serif;
    font-size: 16px;
    font-weight: 400;
}

/* Aplica color blanco a todos los textos */
* {
    color: #FFFFFF;
}

/* === Frames === */
QFrame {
    background-color: #282a36;
    border-radius: 16px;
    border: 1.5px solid #383a42;
    box-shadow: 0 8px 24px 0 rgba(30,34,40,0.08);
}

/* Barra superior */
#frame_superior {
    background: rgba(34,36,42,0.9);
    border: none;
    border-bottom: 1.5px solid #3B3F4B;
    padding: 10px;
}

/* Panel lateral */
#frame_lateral {
    background: rgba(38,41,49,0.96);
    border-right: 1.5px solid #38404B;
    padding-top: 15px;
    padding-bottom: 15px;
}

/* Área principal */
#frame_paginas {
    background: transparent;
}

/* === Botones === */
QPushButton {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #31344b, stop:1 #23253b);
    color: #FFFFFF;
    border: none;
    border-radius: 8px;
    padding: 10px 18px;
    font-weight: 500;
    font-size: 15px;
    letter-spacing: 0.4px;
    transition: 0.2s;
    box-shadow: 0px 1.5px 4px 0 rgba(30,34,40,0.09);
}

QPushButton:hover {
    background: #5e72e4;
    color: #FFFFFF;
    transform: translateY(-2px) scale(1.05);
}

QPushButton:pressed {
    background: #362d7f;
    color: #FFFFFF;
}

/* Botón principal (Menú) */
#boton_menu {
    background: #7F1DFF;
    color: #FFFFFF;
    font-weight: 700;
    letter-spacing: 0.7px;
    box-shadow: 0 4px 16px 0 rgba(127,29,255,0.18);
}
#boton_menu:hover {
    background: #561bac;
}
#boton_menu:pressed {
    background: #3f127a;
}

/* Botón salir */
#boton_salir {
    background: #ff3366;
    color: #FFFFFF;
    border: none;
    font-weight: 600;
    transition: 0.1s;
}
#boton_salir:hover {
    background: #b81342;
}
#boton_salir:pressed {
    background: #79102b;
}

/* === Campos de texto === */
QLineEdit, QTextEdit {
    background: #24252A;
    color: #FFFFFF;
    border: 1.2px solid #6D70F3;
    border-radius: 7px;
    padding: 8px 12px;
    font-size: 15px;
    font-weight: 400;
    transition: border 0.2s;
}
QLineEdit:focus, QTextEdit:focus {
    border: 1.5px solid #7F1DFF;
    background: #353859;
    color: #FFFFFF;
}

/* === Labels === */
QLabel {
    color: #FFFFFF;
    font-weight: 500;
    font-size: 15px;
    letter-spacing: 0.3px;
}

/* Label inferior o secundario */
#label {
    color: #FFFFFF;
    font-size: 13px;
    font-style: italic;
}

/* === Stacked Widget === */
QStackedWidget {
    background: transparent;
    border: 1.2px solid #292b38;
    border-radius: 8px;
    color: #FFFFFF;
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #26282C;
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #5354a6;
    min-height: 24px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #7F1DFF;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 14px;
}

/* === Botones laterales === */
#boton_agregar, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar {
    background: linear-gradient(90deg,#353859 0%,#6D70F3 100%);
    border: none;
    color: #FFFFFF;
    font-weight: 600;
    font-size: 15px;
    margin: 2px 0;
}

#boton_agregar:hover,
#boton_buscar:hover,
#boton_actualizar:hover,
#boton_eliminar:hover,
#boton_consultar:hover {
    background: #7F1DFF;
    color: #FFFFFF;
    border: none;
}

#boton_agregar:pressed,
#boton_buscar:pressed,
#boton_actualizar:pressed,
#boton_eliminar:pressed,
#boton_consultar:pressed {
    background: #561bac;
    color: #FFFFFF;
}

/* === Tooltips === */
QToolTip {
    background: #1a1c23;
    color: #FFFFFF;
    border: 1.5px solid #7F1DFF;
    padding: 7px;
    border-radius: 7px;
    font-size: 14px;
    font-weight: 500;
}
